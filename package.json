{
  "name": "smartive-website-2021",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run develop",
    "develop": "npm run generate && next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.js",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint && npm run type-check",
    "analyze-bundle": "ANALYZE=true npm run build",
    "test": "node test/check-page-errors.js",
    "test:ci": "next dev & node test/check-page-errors.js",
    "deploy:preview": "vercel --token $VERCEL_TOKEN --yes --scope smartive > deployment-url.txt",
    "deploy:production": "vercel --token $VERCEL_TOKEN --confirm --scope smartive --prod",
    "generate": "graphql-codegen"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@notionhq/client": "2.2.13",
    "@smartive/guetzli": "2.10.1",
    "@xstate/fsm": "2.1.0",
    "@xstate/react": "3.2.2",
    "charts.css": "1.1.0",
    "dayjs": "1.11.10",
    "eslint-config-next": "14.0.1",
    "framer-motion": "10.16.4",
    "glider-js": "^1.7.7",
    "image-size": "1.0.2",
    "js-confetti": "0.11.0",
    "mermaid": "10.6.0",
    "motion": "10.16.4",
    "next": "14.0.1",
    "next-plausible": "3.11.3",
    "node-fetch": "3.3.2",
    "prism-react-renderer": "2.1.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-datocms": "^4.1.3",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "9.5.2",
    "react-mailchimp-subscribe": "2.1.3",
    "react-photo-gallery-next": "8.0.1",
    "rokka": "3.15.0",
    "scrollex": "^2.0.1",
    "sharp": "^0.32.6",
    "xstate": "4.38.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typed-document-node": "^5.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/bundle-analyzer": "14.0.0",
    "@smartive/eslint-config": "4.0.1",
    "@smartive/prettier-config": "3.1.2",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@types/mermaid": "9.1.0",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "10.4.16",
    "eslint": "8.52.0",
    "next-sitemap": "4.2.3",
    "playwright": "1.39.0",
    "postcss": "8.4.31",
    "postcss-import": "15.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "wait-on": "^7.0.1"
  }
}
